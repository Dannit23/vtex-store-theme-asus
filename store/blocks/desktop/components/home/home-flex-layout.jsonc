{
    "flex-layout.row#desktop__container-section4":{
        "title": "Flex Desktop Section4",
        "children": [
         "flex-layout.col#desktop__infoCards--left",
         "flex-layout.col#desktop__infoCards--right"
        ],
        "props": {
          "blockClass": "textColumn",
          "preventVerticalStretch": true
        }
      },
